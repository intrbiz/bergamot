<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<bergamot site="bergamot.template">
    
    <command engine="snmp" executor="link-state" name="snmp-link-trap" extends="snmp-trap">
        <parameter description="The SNMP Interface Name"                  name="interface-name"></parameter>
    </command>
    
    <command engine="snmp" executor="generic" name="snmp-generic-trap" extends="snmp-trap" template="yes">
        <parameter description="The trap script"                          name="script"></parameter>
    </command>
    
</bergamot>
