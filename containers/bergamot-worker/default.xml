<worker>
    <cluster>
        <zookeeper>127.0.0.1:2181</zookeeper>
    </cluster>
    <logging level="info"/>
    <!-- Agent Configuration -->
    <engine name="agent" enabled="true"/>
    <parameter name="agent-port">15080</parameter>
    <!-- SNMP Configuration -->
    <engine name="snmp" enabled="true"/>
    <parameter name="snmp-port">8161</parameter>
</worker>
